				android:layout_height="match_parent">
				<Button 
					android:gravity="center" 
					android:textSize="16dip" 
					android:layout_width="100px" 
					android:layout_height="60px" android:id="@+id/submit" android:text="@string/submit">
				</Button>
			</LinearLayout>
			<LinearLayout 
				android:layout_width="wrap_content" 
				android:layout_height="match_parent"
				android:paddingLeft="50px">
				<Button 
					android:gravity="center" 
					android:textSize="16dip" 
					android:layout_width="100px" 
					android:layout_height="60px" android:text="@string/reset" android:id="@+id/reset">
				</Button>
			</LinearLayout>
